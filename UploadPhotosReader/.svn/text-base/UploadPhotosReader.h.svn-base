#import <Foundation/Foundation.h>
#import "SBJSON.h"

@protocol UploadPhotosDelegate <NSObject>

-(void)uploadPhotosSuccessful:(NSString*)message;
-(void)uploadPhotosFailed:(NSString*)error;

@end

@interface UploadPhotosReader : NSObject {
    
    NSURLConnection     *urlConnection;
    NSMutableData       *urlData;
}

@property (nonatomic, assign) id<UploadPhotosDelegate>    delegate;

+(UploadPhotosReader*)sharedInstance;
-(void)requestUploadPhotos:(NSDictionary*)uploadData Delegate:(id<UploadPhotosDelegate>)uploadDelegate;
-(void)parseData:(NSString*)responseData;

@end