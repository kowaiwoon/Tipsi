#import <Foundation/Foundation.h>
#import "UserInfo.h"

@protocol SignUpDelegate <NSObject>

-(void)signUpSuccessful:(NSString*)message;
-(void)signUpFailure:(NSString*)error;

@end

@interface SignUpReader : NSObject {
    NSURLConnection     *urlConnection;
    NSMutableData       *urlData;
}

@property (nonatomic, assign) id<SignUpDelegate>      delegate;

+(SignUpReader*)sharedInstance;
-(void)requestSignUp:(NSDictionary*)userInfo Delegate:(id<SignUpDelegate>)signUpDelegate;
-(void)parseData:(NSString*)responseData;

@end
